@model IEnumerable<Integratie.MVC.Models.CSV>

@{
    ViewBag.Title = "CSV";
}

<h2>CSV</h2>
@using(Html.BeginForm("Upload_file", "Home", FormMethod.Post, new {
    enctype = "multipart/form-data"
}))
{
    <label for="thumbnail">Upload csv</label>
    <input type="file" name="file" accept=".csv" />
    <input type="submit" class="btn btn-success" value="upload" />
}
<p>
    @Html.ActionLink("Create New", "Create")
</p>

